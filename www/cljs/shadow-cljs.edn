{:builds
 {:app {:target :browser
        :compiler-options
        {:infer-externs true
         :externs ["cherry-externs.js"]}

        :output-dir "build"

        :modules
        {:main {:init-fn net.thewagner.cherry/init}}

        :js-options
        {:js-provider :external
         :js-package-dirs ["node_modules", "../../node_modules"]
         :external-index "build/index.js"}

        :build-hooks
        [(net.thewagner.build/hook)]}}}
